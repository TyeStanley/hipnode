{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "prisma/seed.ts",
    "prisma/seed/index.ts",
    "prisma/seed/createUsers.js",
    "prisma/seed/createTags.js",
    "prisma/seed/createPosts.js",
    "prisma/seed/createOnboarding.js",
    "lib/prisma.ts",
    "app/(root)/post/[id]/page.jsx",
    "components/posts/create-post-form/Location.tsx"
  ],
  "exclude": ["node_modules"]
}
